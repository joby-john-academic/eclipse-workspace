package com.reverseDigits;

public class Reverser {

	public static void main(String[] args) {
		int num = 12345;
		int reverse = 0;
		int digit;
		while(num>10) {
			digit = num%10;
			num = num/10;
			reverse += digit;
			reverse *=10;
			System.out.println(num);
			
		}
		System.out.println(reverse);

	}

}
